{
    "name": "darkwebdesign/symfony-addon-transformers",
    "type": "library",
    "description": "Symfony Add-on Transformers",
    "keywords": ["symfony", "transformers"],
    "homepage": "http://www.darkwebdesign.nl/",
    "license": "MIT",
    "authors": [
        {"name": "Raymond Schouten"}
    ],
    "require": {
        "doctrine/common": "^2.4",
        "doctrine/orm": "^2.4",
        "php": ">=5.5.9",
        "symfony/form": "3.4.*"
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "^0.3.2",
        "phpunit/phpunit": "^4.8"
    },
    "suggest": {
        "darkwebdesign/symfony-addon-pack": "All Symfony add-ons bundled together",
        "darkwebdesign/symfony-form-types": "Enables the use of the EntityType Form Field"
    },
    "autoload": {
        "psr-4": {
            "DarkWebDesign\\SymfonyAddonTransformers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DarkWebDesign\\SymfonyAddonTransformers\\Tests\\": "tests/"
        }
    }
}
