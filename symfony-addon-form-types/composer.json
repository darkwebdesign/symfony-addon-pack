{
    "name": "darkwebdesign/symfony-addon-form-types",
    "type": "library",
    "description": "Symfony Add-on Form Types",
    "keywords": ["form", "symfony", "types"],
    "homepage": "http://www.darkwebdesign.nl/",
    "license": "MIT",
    "authors": [
        {"name": "Raymond Schouten"}
    ],
    "require": {
        "darkwebdesign/symfony-addon-transformers": "3.0.*",
        "doctrine/common": "^2.4",
        "php": ">=5.5.9",
        "symfony/form": "3.0.*"
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "^0.3.2",
        "phpunit/phpunit": "^4.8"
    },
    "suggest": {
        "darkwebdesign/symfony-addon-pack": "All Symfony add-ons bundled together"
    },
    "autoload": {
        "psr-4": {
            "DarkWebDesign\\SymfonyAddon\\FormType\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DarkWebDesign\\SymfonyAddon\\FormType\\Tests\\": "tests/"
        }
    }
}
