{
    "name": "darkwebdesign/symfony-addon-validators",
    "type": "library",
    "description": "Symfony Add-on Validators",
    "keywords": ["constraints", "symfony", "validators"],
    "homepage": "http://www.darkwebdesign.nl/",
    "license": "MIT",
    "authors": [
        {"name": "Raymond Schouten"}
    ],
    "require": {
        "php": "^5.3.3",
        "symfony/validator": "2.3.*"
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "^0.3.2",
        "phpunit/phpunit": "^4.8"
    },
    "suggest": {
        "darkwebdesign/symfony-addon-pack": "All Symfony add-ons bundled together"
    },
    "autoload": {
        "psr-4": {
            "DarkWebDesign\\SymfonyAddon\\Validator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DarkWebDesign\\SymfonyAddon\\Validator\\Tests\\": "tests/"
        }
    }
}
